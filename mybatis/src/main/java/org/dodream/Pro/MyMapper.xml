<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper

PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.dodream.Pro.MyDAO"> <!--이 sql 문장과 매핑될 인터페이스의 완전한 경로-->

    <insert id="join" parameterType="org.dodream.Pro.MyVO">
        insert into puser  
        	values ( puser_seq.nextval, #{name}, #{id}, #{pwd}, #{phone},#{gender},#{email},'user','1')
    </insert>
    
</mapper>

